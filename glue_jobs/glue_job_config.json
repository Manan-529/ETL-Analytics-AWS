{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents",
				"glue:GetDatabase",
				"glue:GetDatabases",
				"glue:CreateTable",
				"glue:UpdateTable",
				"glue:DeleteTable",
				"glue:GetTable",
				"glue:GetTables",
				"glue:GetTableVersion",
				"glue:GetTableVersions"
			],
			"Resource": [
				"arn:aws:logs:us-east-2:381492195188:log-group:/aws-glue/*",
				"arn:aws:logs:us-east-2:381492195188:log-group:/aws-glue/*:log-stream:*",
				"arn:aws:glue:us-east-2:381492195188:catalog",
				"arn:aws:glue:us-east-2:381492195188:database/*",
				"arn:aws:glue:us-east-2:381492195188:table/*/*"
			]
		}
	]
}
